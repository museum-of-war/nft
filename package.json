{
  "name": "ukraine-nft",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "test": "hardhat test",
    "build": "yarn run compile && yarn run typechain:all",
    "compile": "hardhat compile",
    "typechain:all": "yarn run typechain:local && yarn run typechain:uni-core && yarn run typechain:uni-periphery",
    "typechain:local": "typechain --target ethers-v5 --outDir typechain/external './abi/external/**/*.json'",
    "farm": "HARDHAT_NETWORK=matic ts-node --files --project=./tsconfig.json scripts/farm.ts"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/hardhat-upgrades": "^1.8.2",
    "@symblox/hardhat-abi-gen": "^0.2.0",
    "@typechain/ethers-v5": "^6.0.5",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.1",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.1.4",
    "hardhat": "^2.2.1",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "hardhat-typechain": "^0.3.5",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/contracts-upgradeable": "^4.1.0",
    "dotenv": "^9.0.2"
  }
}
